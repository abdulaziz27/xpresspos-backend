apiVersion: v1
kind: Secret
metadata:
  name: xpresspos-backend-secrets
  namespace: xpresspos
type: Opaque
stringData:
  APP_KEY: <APP_KEY>
  DB_HOST: <DB_HOST>
  DB_PORT: "3306"
  DB_DATABASE: <DB_NAME>
  DB_USERNAME: <DB_USER>
  DB_PASSWORD: <DB_PASSWORD>
  MAIL_HOST: <SMTP_HOST>
  MAIL_PORT: <SMTP_PORT>
  MAIL_USERNAME: <SMTP_USERNAME>
  MAIL_PASSWORD: <SMTP_PASSWORD>
  MAIL_ENCRYPTION: <tls_or_ssl>
  MAIL_FROM_ADDRESS: no-reply@xpresspos.id
  MAIL_FROM_NAME: XpressPOS
  MIDTRANS_SERVER_KEY: ""
  MIDTRANS_CLIENT_KEY: ""
  MIDTRANS_PARTNER_CODE: ""
  SENTRY_LARAVEL_DSN: ""
